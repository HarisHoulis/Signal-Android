<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/checkbox_corner_radius"/>
            <stroke android:width="@dimen/stroke_regular" android:color="@color/content"/>
            <size android:height="@dimen/checkbox_size" android:width="@dimen/checkbox_size"/>
        </shape>
    </item>
    <item android:top="@dimen/spacing_xs" android:bottom="@dimen/spacing_xs" android:left="@dimen/spacing_xs" android:right="@dimen/spacing_xs">
        <shape android:shape="rectangle">
            <solid android:color="@color/content"/>
            <corners android:radius="@dimen/checkbox_corner_radius"/>
        </shape>
    </item>
</layer-list>
